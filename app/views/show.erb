<h1 class="text-center">Statistics for: <br></h1>
<h3 class= "text-center"><%= @u %></h3>
<div class="row well text-center">
  <div class="col-md-6">
    <h2>Response Data:</h2>
    <p><strong> Maximum: </strong><%= @url_id.max_response_time %> </p>
    <p><strong> Minimum: </strong><%= @url_id.min_response_time %> </p>
    <p><strong> All Times: </strong><%= @url_id.all_response_times %> </p>
    <p><strong> Average: </strong><%= @url_id.average_response_time %> </p>
  </div>

  <div class="col-md-6">
    <h2>Other:</h2>
    <p><strong> Associated HTTP Verbs: </strong><%= @url_id.all_http_verbs %> </p>
    <p><strong> Most Popular Referrers: </strong><%= @url_id.top_three_referrers %> </p>
    <p><strong> Top User Agents: </strong><%= @url_id.top_three_user_agents %> </p>
  </div>
</div>
